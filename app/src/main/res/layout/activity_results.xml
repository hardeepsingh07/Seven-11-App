<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:background="@color/black"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.singh.seven_11.Results"
    android:gravity="center">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="75dp"
        android:layout_height="50dp"
        android:src="@drawable/seveneleven"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Reports"
        android:textColor="#ffffff"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="25sp"
        android:textStyle="italic"
        android:typeface="serif"
        android:layout_alignBottom="@+id/imageView6"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="75dp"
        android:layout_height="50dp"
        android:src="@drawable/seveneleven"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Lottery Report"
        android:id="@+id/textView30"
        android:layout_marginTop="40dp"
        android:typeface="serif"
        android:textStyle="bold"
        android:textSize="20sp"
        android:layout_below="@+id/textView19"
        android:layout_toStartOf="@+id/LottoSale"
        android:textColor="#ffffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Lotto Sale"
        android:id="@+id/textView31"
        android:typeface="serif"
        android:layout_below="@+id/textView30"
        android:layout_toRightOf="@+id/imageView6"
        android:layout_toEndOf="@+id/imageView6"
        android:textColor="#ffffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Lotto Win"
        android:id="@+id/textView32"
        android:typeface="serif"
        android:textColor="#ffffff"
        android:layout_below="@+id/LottoSale"
        android:layout_alignLeft="@+id/textView31"
        android:layout_alignStart="@+id/textView31" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Scratcher Sale"
        android:id="@+id/textView33"
        android:typeface="serif"
        android:textColor="#ffffff"
        android:layout_below="@+id/LottoWin"
        android:layout_alignRight="@+id/textView30"
        android:layout_alignEnd="@+id/textView30" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Scratcher Win"
        android:id="@+id/textView34"
        android:typeface="serif"
        android:textColor="#ffffff"
        android:layout_below="@+id/ScractcherSale"
        android:layout_alignLeft="@+id/textView33"
        android:layout_alignStart="@+id/textView33" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/LottoSale"
        android:textColor="#ffffff"
        android:typeface="serif"
        android:textStyle="normal"
        android:layout_alignTop="@+id/textView31"
        android:layout_toRightOf="@+id/textView31"
        android:layout_toEndOf="@+id/textView31"
        android:gravity="center" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/LottoWin"
        android:textColor="#ffffff"
        android:typeface="serif"
        android:textStyle="normal"
        android:layout_alignTop="@+id/textView32"
        android:layout_toRightOf="@+id/textView32"
        android:layout_toEndOf="@+id/textView32"
        android:gravity="center" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/ScractcherSale"
        android:textColor="#ffffff"
        android:typeface="serif"
        android:textStyle="normal"
        android:layout_below="@+id/LottoWin"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignLeft="@+id/LottoWin"
        android:layout_alignStart="@+id/LottoWin"
        android:gravity="center" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/ScratcherWin"
        android:textColor="#ffffff"
        android:typeface="serif"
        android:textStyle="normal"
        android:layout_alignTop="@+id/textView34"
        android:layout_toEndOf="@+id/textView34"
        android:layout_toRightOf="@+id/textView34"
        android:gravity="center" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Invoices -:"
        android:id="@+id/textView35"
        android:typeface="serif"
        android:textStyle="bold"
        android:textSize="20sp"
        android:textColor="#ffffff"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/ListInvoices"
        android:layout_width="200dp"
        android:layout_height="35dp"
        android:background="#ffffff"
        android:text="List of Invoices"
        android:textColor="@color/black"
        android:textStyle="italic"
        android:typeface="serif"
        android:layout_alignTop="@+id/textView35"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_toRightOf="@+id/textView36"
        android:layout_toEndOf="@+id/textView36" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Cash Report"
        android:id="@+id/textView36"
        android:layout_marginTop="28dp"
        android:typeface="serif"
        android:textStyle="bold"
        android:textSize="20sp"
        android:layout_below="@+id/ListInvoices"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#ffffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Front Change"
        android:id="@+id/textView37"
        android:typeface="serif"
        android:layout_below="@+id/textView36"
        android:layout_alignLeft="@+id/textView34"
        android:layout_alignStart="@+id/textView34"
        android:textColor="#ffffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Back Change"
        android:id="@+id/textView38"
        android:typeface="serif"
        android:gravity="center"
        android:textColor="#ffffff"
        android:layout_below="@+id/frontChange"
        android:layout_alignLeft="@+id/textView37"
        android:layout_alignStart="@+id/textView37" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Adjustment"
        android:id="@+id/textView39"
        android:typeface="serif"
        android:gravity="center"
        android:textColor="#ffffff"
        android:layout_alignTop="@+id/Adjustment"
        android:layout_alignRight="@+id/textView40"
        android:layout_alignEnd="@+id/textView40" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Total Cash"
        android:id="@+id/textView40"
        android:typeface="serif"
        android:gravity="center"
        android:textColor="#ffffff"
        android:layout_below="@+id/Adjustment"
        android:layout_alignRight="@+id/textView37"
        android:layout_alignEnd="@+id/textView37" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/frontChange"
        android:textColor="#ffffff"
        android:typeface="serif"
        android:textStyle="normal"
        android:layout_alignTop="@+id/textView37"
        android:layout_toRightOf="@+id/textView34"
        android:layout_toEndOf="@+id/textView34"
        android:gravity="center" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/backChange"
        android:textColor="#ffffff"
        android:typeface="serif"
        android:textStyle="normal"
        android:layout_below="@+id/frontChange"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignLeft="@+id/frontChange"
        android:layout_alignStart="@+id/frontChange"
        android:gravity="center" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/Adjustment"
        android:textColor="#ffffff"
        android:typeface="serif"
        android:textStyle="normal"
        android:layout_below="@+id/backChange"
        android:layout_alignLeft="@+id/backChange"
        android:layout_alignStart="@+id/backChange"
        android:gravity="center" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/totalEndCash"
        android:textColor="#ffffff"
        android:typeface="serif"
        android:textStyle="normal"
        android:layout_below="@+id/Adjustment"
        android:layout_alignLeft="@+id/Adjustment"
        android:layout_alignStart="@+id/Adjustment"
        android:gravity="center" />

    <Button
        android:id="@+id/end"
        android:layout_width="200dp"
        android:layout_height="35dp"
        android:background="@color/black"
        android:text="Done! "
        android:textColor="#ffffff"
        android:textStyle="italic"
        android:typeface="serif"
        android:layout_marginBottom="35dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />


</RelativeLayout>
